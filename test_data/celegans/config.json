{
  "plugins": [
    {
      "name": "MafViewer",
      "url": "http://localhost:9000/dist/out.js"
    }
  ],
  "assemblies": [
    {
      "name": "ce10",
      "sequence": {
        "type": "ReferenceSequenceTrack",
        "trackId": "ce10-ReferenceSequenceTrack",
        "adapter": {
          "type": "BgzipFastaAdapter",
          "fastaLocation": {
            "uri": "chrI.fa.gz"
          },
          "faiLocation": {
            "uri": "chrI.fa.gz.fai"
          },
          "gziLocation": {
            "uri": "chrI.fa.gz.gzi"
          }
        }
      }
    }
  ],
  "defaultSession": {
    "name": "New Session"
  },
  "tracks": [
    {
      "type": "MafTrack",
      "trackId": "chrI.bed",
      "name": "chrI.bed",
      "adapter": {
        "type": "MafTabixAdapter",
        "samples": ["ce10", "cb4", "caeSp111", "caeRem4", "caeJap4", "caePb3"],
        "bedGzLocation": {
          "uri": "chrI.bed.gz"
        },
        "index": {
          "location": {
            "uri": "chrI.bed.gz.tbi"
          }
        }
      },
      "assemblyNames": ["ce10"]
    }
  ]
}
